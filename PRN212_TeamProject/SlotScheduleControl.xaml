<Window x:Class="PRN212_TeamProject.SlotScheduleControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRN212_TeamProject"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        WindowStartupLocation="CenterScreen"
        Title="SlotScheduleControl" 
        Loaded="SlotLoader">
    <Grid>
        <DataGrid x:Name="dgSlot" SelectionChanged="dgSlot_SelectedChanged" Margin="0,0,0,0" Width="600" Height="310"/>
    </Grid>
</Window>
