<UserControl x:Class="PRN212_TeamProject.ViewSheduleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PRN212_TeamProject"
             mc:Ignorable="d" 
            >
    <StackPanel Margin="0,10,0,0">
        <Grid Margin="0,0,0,10">
            <TextBox Height="20" Width="220"  HorizontalAlignment="Left" Margin="180,0,0,0" Name="txtSearch" />
            <Button Height="20" Width="60" Margin="380,0,0,0" Content="Search" Click="btnSearch_Click"/>
        </Grid>
        <local:BookingTable x:Name="bookingTableControl" Height="280"/>
    </StackPanel>
</UserControl>
